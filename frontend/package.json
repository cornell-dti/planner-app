{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build:stage": "env-cmd -f .env.staging yarn react-scripts build",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "optimize-svg": "svgo --disable=removeViewBox --enable=prefixIds,removeDimensions src/assets/svgs/*.svg",
    "tsc": "tsc -p tsconfig.json",
    "check-all": "yarn tsc && yarn test"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "common": "0.1.0",
    "firebase": "^7.8.0",
    "fuse.js": "^3.4.6",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.12.0",
    "react-beautiful-dnd": "^12.2.0",
    "react-calendar": "^2.19.2",
    "react-color": "^2.18.0",
    "react-dev-utils": "^10.1.0",
    "react-dom": "^16.12.0",
    "react-dom-confetti": "^0.1.3",
    "react-firebaseui": "^4.1.0",
    "react-modal": "^3.11.1",
    "react-redux": "^7.1.3",
    "react-test-renderer": "^16.12.0",
    "redux": "^4.0.5",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.9",
    "@types/react-beautiful-dnd": "^12.1.0",
    "@types/react-color": "^3.0.1",
    "@types/react-dom": "^16.9.5",
    "@types/react-modal": "^3.10.4",
    "@types/react-redux": "7.1.3",
    "@types/react-test-renderer": "^16.9.2",
    "env-cmd": "^10.1.0",
    "node-sass": "^4.13.1",
    "react-scripts": "^3.3.1",
    "svgo": "^1.3.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
