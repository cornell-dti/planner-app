{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next",
    "build:stage": "env-cmd -f .env.staging next build && env-cmd -f .env.staging next export",
    "build": "next build && next export",
    "optimize-svg": "svgo --disable=removeViewBox --enable=prefixIds,removeDimensions src/assets/svgs/*.svg",
    "tsc": "tsc -p tsconfig.json",
    "check-all": "yarn tsc && yarn test"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "common": "0.1.0",
    "firebase": "^7.21.0",
    "fuse.js": "^6.4.1",
    "immutable": "^4.0.0-rc.12",
    "next": "^9.5.3",
    "next-transpile-modules": "^4.1.0",
    "react": "^16.14.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-calendar": "^3.1.0",
    "react-color": "^2.18.1",
    "react-dom": "^16.14.0",
    "react-dom-confetti": "^0.2.0",
    "react-firebaseui": "^4.1.0",
    "react-modal": "^3.11.1",
    "react-redux": "^7.2.1",
    "react-test-renderer": "^16.14.0",
    "redux": "^4.0.5",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@types/react": "16.9.46",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-calendar": "^3.1.1",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^16.9.8",
    "@types/react-modal": "^3.10.6",
    "@types/react-redux": "7.1.3",
    "@types/react-test-renderer": "^16.9.3",
    "env-cmd": "^10.1.0",
    "node-sass": "^4.13.1",
    "svgo": "^1.3.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
